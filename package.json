  },
  "devDependencies": {
    "chai": "^3.5",
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.5",
    "mocha": "^2.4",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dittto/serverless-package-includes.git"
  },
  "keywords": [
    "serverless",
    "package",
    "includes"
  ],
  "author": "Nick Stacey @ditt_to",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dittto/serverless-package-includes/issues"
  },
  "homepage": "https://github.com/dittto/serverless-package-includes#readme",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
